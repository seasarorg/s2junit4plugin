<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <viewerContribution
            id="org.seasar.s2junit4plugin.viewerContribution"
            targetID="#CompilationUnitEditorContext">
         <menu
               id="org.seasar.s2junit4plugin.viewerContribution.menu"
               label="S2JUnit4"
               path="additions">
            <separator
                  name="additions">
            </separator>
         </menu>
         <action
               class="org.seasar.s2junit4plugin.action.JUnitDebugAction"
               definitionId="org.seasar.s2junit4plugin.command.JUnitDebugAction"
               id="org.seasar.s2junit4plugin.action.JUnitDebugAction"
               label="%JUnitDebugAction.label"
               menubarPath="org.seasar.s2junit4plugin.viewerContribution.menu/additions">
         </action>
         <action
               class="org.seasar.s2junit4plugin.action.JUnitRunAction"
               definitionId="org.seasar.s2junit4plugin.command.JUnitRunAction"
               id="org.seasar.s2junit4plugin.action.JUnitRunAction"
               label="%JUnitRunAction.label"
               menubarPath="org.seasar.s2junit4plugin.viewerContribution.menu/additions">
         </action>
         <action
               class="org.seasar.s2junit4plugin.action.OpenTestingPairAction"
               definitionId="org.seasar.s2junit4plugin.command.OpenTestingPairAction"
               id="org.seasar.s2junit4plugin.action.OpenTestingPairAction"
               label="%OpenTestingPairAction.label"
               menubarPath="org.seasar.s2junit4plugin.viewerContribution.menu/additions">
         </action>
      </viewerContribution>
      <objectContribution
            adaptable="true"
            id="org.seasar.s2junit4plugin.objectContribution"
            objectClass="org.eclipse.jdt.core.IJavaElement">
         <menu
               id="org.seasar.s2junit4plugin.objectContribution.menu"
               label="S2JUnit4"
               path="additions">
            <separator
                  name="additions">
            </separator>
         </menu>
         <action
               class="org.seasar.s2junit4plugin.action.JUnitDebugAction"
               definitionId="org.seasar.s2junit4plugin.command.JUnitDebugAction"
               id="org.seasar.s2junit4plugin.action.JUnitDebugAction"
               label="%JUnitDebugAction.label"
               menubarPath="org.seasar.s2junit4plugin.objectContribution.menu/additions">
         </action>
         <action
               class="org.seasar.s2junit4plugin.action.JUnitRunAction"
               definitionId="org.seasar.s2junit4plugin.command.JUnitRunAction"
               id="org.seasar.s2junit4plugin.action.JUnitRunAction"
               label="%JUnitRunAction.label"
               menubarPath="org.seasar.s2junit4plugin.objectContribution.menu/additions">
         </action>
         <action
               class="org.seasar.s2junit4plugin.action.OpenTestingPairAction"
               definitionId="org.seasar.s2junit4plugin.command.OpenTestingPairAction"
               id="org.seasar.s2junit4plugin.action.OpenTestingPairAction"
               label="%OpenTestingPairAction.label"
               menubarPath="org.seasar.s2junit4plugin.objectContribution.menu/additions">
               <enablement>
                  <or>
                  <objectClass
                        name="org.eclipse.jdt.core.IMember">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.jdt.core.ICompilationUnit">
                  </objectClass>
               </or>
               </enablement>
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.seasar.s2junit4plugin.command.OpenTestingPairAction"
            contextId="org.eclipse.jdt.ui.javaEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+9">
      </key>
      <key
            commandId="org.seasar.s2junit4plugin.command.JUnitRunAction"
            contextId="org.eclipse.jdt.ui.javaEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+0">
      </key>
      <key
            commandId="org.seasar.s2junit4plugin.command.JUnitDebugAction"
            contextId="org.eclipse.jdt.ui.javaEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+0">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            description="%Category.description"
            id="org.seasar.s2junit4plugin.category"
            name="%Category.name">
      </category>
      <command
            categoryId="org.seasar.s2junit4plugin.category"
            description="%OpenTestingPairAction.description"
            id="org.seasar.s2junit4plugin.command.OpenTestingPairAction"
            name="%OpenTestingPairAction.label">
      </command>
      <command
            categoryId="org.seasar.s2junit4plugin.category"
            description="%JUnitRunAction.description"
            id="org.seasar.s2junit4plugin.command.JUnitRunAction"
            name="%JUnitRunAction.label">
      </command>
      <command
            categoryId="org.seasar.s2junit4plugin.category"
            description="%JUnitDebugAction.description"
            id="org.seasar.s2junit4plugin.command.JUnitDebugAction"
            name="%JUnitDebugAction.label">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="org.seasar.s2junit4plugin.editorContribution"
            targetID="org.eclipse.jdt.ui.CompilationUnitEditor">

         <menu
               id="org.seasar.s2junit4plugin.editorContribution.menu"
               label="S2JUnit4"
               path="Edit/additions">
            <separator
                  name="additions">
            </separator>
         </menu>
         <action
               class="org.seasar.s2junit4plugin.action.JUnitDebugAction"
               definitionId="org.seasar.s2junit4plugin.command.JUnitDebugAction"
               id="org.seasar.s2junit4plugin.action.JUnitDebugAction"
               label="%JUnitDebugAction.label"
               menubarPath="Edit/org.seasar.s2junit4plugin.editorContribution.menu/additions">
         </action>
         <action
               class="org.seasar.s2junit4plugin.action.JUnitRunAction"
               definitionId="org.seasar.s2junit4plugin.command.JUnitRunAction"
               id="org.seasar.s2junit4plugin.action.JUnitRunAction"
               label="%JUnitRunAction.label"
               menubarPath="Edit/org.seasar.s2junit4plugin.editorContribution.menu/additions">
         </action>
         <action
               class="org.seasar.s2junit4plugin.action.OpenTestingPairAction"
               definitionId="org.seasar.s2junit4plugin.command.OpenTestingPairAction"
               id="org.seasar.s2junit4plugin.action.OpenTestingPairAction"
               label="%OpenTestingPairAction.label"
               menubarPath="Edit/org.seasar.s2junit4plugin.editorContribution.menu/additions">
         </action>

      </editorContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.jdt.ui.preferences.JavaBasePreferencePage"
            class="org.seasar.s2junit4plugin.preference.QuickJUnitPreferencePage"
            id="org.seasar.s2junit4plugin.page.QuickJUnitPreferencePage"
            name="S2JUnit4">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="org.seasar.s2junit4plugin.property.QuickJUnitInfoPage"
            id="org.seasar.s2junit4plugin.page.QuickJUnitInfoPage"
            name="S2JUnit4">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.seasar.s2junit4plugin.preference.S2JUnit4PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
